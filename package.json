{
  "name": "terra-dev-site",
  "version": "4.0.0",
  "description": "Dynamically builds a react-hash-routed site based on site configuration, navigation configuration and component configuration.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-dev-site"
  },
  "keywords": [
    "Cerner",
    "Terra",
    "Terra Dev Site",
    "terra-dev-site",
    "UI"
  ],
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-dev-site/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "/src/**.js",
      "/src/**.jsx"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "github-markdown-css": "identity-obj-proxy"
    }
  },
  "bin": {
    "tds-generate-app-config": "bin/generateAppConfig.js"
  },
  "eslintConfig": {
    "extends": "terra"
  },
  "stylelint": {
    "extends": "stylelint-config-terra"
  },
  "scripts": {
    "clean": "node scripts/clean-modules/index.js",
    "clean:compiled": "rimraf lib",
    "clean:install": "npm run clean && npm install",
    "clean:obsolete-snapshots": "npm test -- -u",
    "compile": "npm run clean:compiled && npm run compile:build",
    "compile:build": "babel src --out-dir lib --copy-files",
    "compile:dev": "webpack --config config/webpack/webpack.config",
    "compile:prod": "webpack --config config/webpack/webpack.config -p",
    "danger": "danger ci",
    "heroku-postbuild": "npm install --only=dev && npm run compile:prod",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:scss": "stylelint src/**/*.scss",
    "prepare": "npm run compile",
    "prepublishOnly": "npm test",
    "pretest": "npm run lint",
    "release:major": "node scripts/release/release.js major",
    "release:minor": "node scripts/release/release.js minor",
    "release:patch": "node scripts/release/release.js patch",
    "start": "tt-serve --config config/webpack/webpack.config",
    "start-heroku": "npm run compile:prod && tt-serve-static --port $PORT --site './build' ",
    "start-static": "npm run compile:prod && tt-serve-static --site ./build",
    "test": "npm run jest",
    "tds:generate-app-config": "node --inspect-brk bin/generateAppConfig.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "commander": "^2.9.0",
    "escodegen": "^1.9.1",
    "focus-trap-react": "4.0.0",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.1",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "prop-types": "^15.5.8",
    "react-intl": "^2.4.0",
    "terra-action-footer": "^2.3.0",
    "terra-action-header": "^2.10.0",
    "terra-application-navigation": "https://github.cerner.com/terra/terra-snapshots/raw/master/terra-framework/application-navigation/terra-application-navigation.tgz",
    "terra-application": "https://github.cerner.com/terra/terra-snapshots/raw/master/terra-framework/application-navigation/terra-application.tgz",
    "terra-button-group": "^3.5.0",
    "terra-button": "^3.9.0",
    "terra-content-container": "^3.6.0",
    "terra-doc-template": "^2.7.0",
    "terra-form-select": "^5.6.0",
    "terra-icon": "^3.6.0",
    "terra-image": "^3.6.0",
    "terra-markdown": "^2.25.0",
    "terra-overlay": "^3.11.0",
    "terra-spacer": "^3.3.0",
    "terra-status-view": "^3.5.0",
    "to-ast": "^1.0.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^3.1.0",
    "react-dom": "^16.8.5",
    "react-router-dom": "^4.2.2",
    "react": "^16.8.5",
    "terra-toolkit": "git+https://github.com/cerner/terra-toolkit.git#serve-updates",
    "webpack-merge": "^4.1.2",
    "webpack": "^4.28.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "danger": "^3.7.14",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "enzyme": "^3.3.0",
    "eslint-config-terra": "^2.1.0",
    "eslint": "^5.0.0",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^23.1.0",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.2",
    "react": "^16.4.2",
    "shelljs": "^0.8.2",
    "stylelint-config-terra": "^1.2.0",
    "stylelint": "^9.0.0",
    "terra-toolkit": "git+https://github.com/cerner/terra-toolkit.git#serve-updates",
    "webpack-merge": "^4.1.2",
    "webpack": "^4.28.1"
  }
}
